<Page
    x:Class="WpfApp3.View.MediumLabel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfApp3.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    xmlns:syncfusionskin="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
    Title="MediumLabel"
    d:DesignHeight="188.97"
    d:DesignWidth="226.77"
    Background="White"
    mc:Ignorable="d">


    <Grid>
        <StackPanel x:Name="stackPanel">
            <Grid
                x:Name="Grid"
                Height="160"
                Margin="0,0,0,0"
                HorizontalAlignment="Center">
                <!--  Define Rows for each ComboBox & TextBox  -->
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <!--  Define Columns for ComboBox, Label, and TextBox  -->
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox1"
                    Grid.Row="0"
                    Grid.RowSpan="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="7"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Bottom"
                    BorderThickness="0"
                    FontSize="12"
                    Text="Natural Valencia"
                    Watermark="Natural Valencia" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox2"
                    Grid.Row="1"
                    Grid.RowSpan="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="7"
                    HorizontalAlignment="Center"
                    BorderThickness="0"
                    FontSize="12"
                    Text="Oren Valencia"
                    TextWrapping="Wrap"
                    Watermark="Oren Valencia" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox3"
                    Grid.Row="1"
                    Grid.RowSpan="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="7"
                    Margin="0,0,0,4"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Bottom"
                    BorderThickness="0"
                    FontSize="9"
                    Text="瓦伦西亚橙"
                    TextWrapping="Wrap"
                    Watermark="瓦伦西亚橙" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox4"
                    Grid.Row="2"
                    Grid.RowSpan="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    FontSize="9"
                    Text="Produce of Egypt"
                    TextWrapping="Wrap"
                    Watermark="Produce of Egypt" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox5"
                    Grid.Row="2"
                    Grid.RowSpan="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Margin="0,12,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    FontSize="9"
                    Text="Hasil Egypt"
                    TextWrapping="Wrap"
                    Watermark="Hasil Egypt" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox6"
                    Grid.Row="2"
                    Grid.RowSpan="2"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Margin="0,25,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    FontSize="15"
                    Text="RM6.94"
                    TextWrapping="Wrap"
                    Watermark="RM6.94" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox7"
                    Grid.Row="2"
                    Grid.RowSpan="2"
                    Grid.Column="3"
                    Grid.ColumnSpan="3"
                    Margin="0,30,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    FontSize="9"
                    Text="(694gm)"
                    TextWrapping="Wrap"
                    Watermark="(694gm)" />

                <Image
                    x:Name="Image1"
                    Grid.Row="3"
                    Grid.RowSpan="2"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Width="50"
                    Height="50"
                    Margin="0,13,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Source="{StaticResource Img_qrcode}" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox8"
                    Grid.Row="3"
                    Grid.RowSpan="2"
                    Grid.Column="2"
                    Grid.ColumnSpan="3"
                    Margin="0,20,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    FontSize="9"
                    Text="HAHG"
                    TextWrapping="Wrap"
                    Watermark="HAHG" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox9"
                    Grid.Row="4"
                    Grid.RowSpan="1"
                    Grid.Column="2"
                    Grid.ColumnSpan="4"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="Transparent"
                    BorderThickness="0"
                    FontSize="7"
                    Text="Grade Premium Size Mixed"
                    Watermark="Grade Premium Size Mixed" />

                <syncfusion:SfTextBoxExt
                    x:Name="TextBox10"
                    Grid.Row="4"
                    Grid.RowSpan="1"
                    Grid.Column="2"
                    Grid.ColumnSpan="4"
                    Margin="0,10,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="Transparent"
                    BorderThickness="0"
                    FontSize="7"
                    Text="Gred Premium Saiz Campur"
                    Watermark="Gred Premium Saiz Campur" />

                <Image
                    x:Name="Image4"
                    Grid.Row="1"
                    Grid.RowSpan="4"
                    Grid.Column="6"
                    Grid.ColumnSpan="2"
                    Width="38"
                    Height="110"
                    Margin="10,10,0,0"
                    VerticalAlignment="Top"
                    Source="{StaticResource Img_barcode}" />
            </Grid>
        </StackPanel>
    </Grid>
</Page>
